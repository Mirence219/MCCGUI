+        {
+            string text = File.ReadLines(filePath).ElementAt(lineNumber);
+            int startQuote = text.IndexOf('"') + 1; 
+            int endQuote = text.LastIndexOf('"');   
+
+            if (startQuote > 0 && endQuote > startQuote)
